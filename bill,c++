#include<stdio.h>
#include<string.h>

int main(){
	char name[10] [50];
	int bill [10][5];
	int total[10];
	float average[10];
	int i,j;
	int maxtotal =-1, mintotal=9999999;
	char maxname[50];
char minname[50];
	
	for(i=0; i<10; i++) {
		
		total[i] =0;
		
		printf("\n Enter the name of a  customer %d: ",i+1);
		scanf("%s", name[i]);
		
		printf("Enter the  5 months electrecity bill:\n");
		
		for (j=0; j<5; j++) {
			scanf("%d", &bill[i][j]);
			
			
			if(bill[i][j] <0) {
				printf("Invalid bill Enter again ");
				j--;
				continue;
			}
			
			total[i]+=bill[i][j];
					}
					average[i] =total[i]/5.0;
					
					
					if(total[i]> maxtotal){
						maxtotal= total[i];
						strcpy(maxname, name[i]);
					}
					if(total[i]< mintotal){
						mintotal =total[i];
						strcpy(minname ,name[i]);
						
					}
					
					
	}
	
	printf("Highest Total Bill : %s = %d\n", maxname, maxtotal);
    printf("Lowest Total Bill  : %s = %d\n", minname, mintotal);
}
